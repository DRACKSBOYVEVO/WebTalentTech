@model IEnumerable<Web.Models.Voto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Encuesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LiderSocial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaVoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cedula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProyectoSocial)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Encuesta.Apellidos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LiderSocial.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaVoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProyectoSocial.LiderSocialId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VotoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VotoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VotoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
